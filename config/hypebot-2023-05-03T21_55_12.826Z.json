{
  "net": {
    "inputSize": 10,
    "outputSize": 2,
    "binaryThresh": 0.5,
    "activation": "leaky-relu",
    "hiddenLayers": [
      9,
      8,
      7,
      6,
      5,
      4,
      3
    ],
    "leakyReluAlpha": 0.001
  },
  "train": {
    "timeout": 9999999999,
    "iterations": 120000,
    "errorThresh": 0.005,
    "log": true,
    "logPeriod": 5000,
    "learningRate": 0.001,
    "momentum": 0.1,
    "callbackPeriod": 10
  }
}